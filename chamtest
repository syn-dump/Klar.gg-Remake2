-- Scripts

local function PXSLTK_fake_script() -- visual.LocalScript 
	local script = Instance.new('LocalScript', visual)

	visual.MouseButton1Click:Connect(function()
		visuals_frame.Visible = true
	end)
end
coroutine.wrap(PXSLTK_fake_script)()
local function UNIGIE_fake_script() -- settings.LocalScript 
	local script = Instance.new('LocalScript', settings)

	settings.MouseButton1Click:Connect(function()
		settings_frame.Visible = true
	end)
end
coroutine.wrap(UNIGIE_fake_script)()
local function KOBGL_fake_script() -- aimbot_frame.LocalScript 
	local script = Instance.new('LocalScript', aimbot_frame)

	settings.MouseButton1Click:Connect(function()
		setting.Visible = true
	end)
	
	
end
coroutine.wrap(KOBGL_fake_script)()
local function PFQBZ_fake_script() -- chamclick.LocalScript 
	local script = Instance.new('LocalScript', chamclick)

	chamclick.MouseButton1Click:Connect(function()
		loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/syn-dump/Klar.gg-Remake2/main/chamtest"))()
	end)
end
coroutine.wrap(PFQBZ_fake_script)()
